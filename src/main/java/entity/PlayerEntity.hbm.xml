<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.PlayerEntity" table="player" schema="gaysys">
        <id name="playerId">
            <column name="player_id" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="age">
            <column name="age" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="event">
            <column name="event" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="idCard">
            <column name="id_card" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(255)"/>
        </property>
        <property name="teamName">
            <column name="team_name" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>